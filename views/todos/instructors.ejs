<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>First Express</title>
</head>
<body>

    <h1>All Instructors</h1>
    <ul>
        <% instructors.forEach(function(i){ %>
            <li>
                <%= i.name %>
                -
                <%= i.title %>
            </li>
        <% }) %>
    </ul>
</body>
</html>